{
	"name": "@mangoweb/contember-plugins",
	"version": "0.8.6",
	"description": "Common contember plugins.",
	"files": [
		"dist"
	],
	"type": "module",
	"main": "dist/commonjs/index.js",
	"module": "dist/index.js",
	"scripts": {
		"test": "npm run eslint:lint",
		"eslint:lint": "eslint \"src/**/*.{ts,tsx}\"",
		"eslint:fix": "eslint --fix \"src/**/*.{ts,tsx}\"",
		"prepare": "npm run build",
		"prepublishOnly": "npm test",
		"clean": "rimraf dist/",
		"build": "npm run clean && npm run build:esm && npm run build:commonjs && npm run copy-sass-to-dist",
		"build:esm": "tsc",
		"build:commonjs": "tsc --project tsconfig-commonjs.json && echo { \\\"type\\\": \\\"commonjs\\\" } > dist/commonjs/package.json",
		"copy-sass-to-dist": "cp src/translations/admin/theme.sass dist/translations/admin/theme.sass && cp src/translations/admin/theme.sass dist/commonjs/translations/admin/theme.sass",
		"dev": "tsc --watch"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/manGoweb/contember-plugins.git"
	},
	"publishConfig": {
		"registry": "https://npm.pkg.github.com"
	},
	"peerDependencies": {
		"@contember/admin": "^1.0.0-rc.27",
		"@contember/schema-definition": "^1.1.0-alpha.5 || ^0.12.0-alpha.10 || ^1.0.0-rc.16",
		"react": "^17"
	},
	"devDependencies": {
		"@contember/admin": "^1.0.0-rc.27",
		"@contember/schema-definition": "^1.1.0-alpha.5",
		"@types/react": "17.0.42",
		"@typescript-eslint/eslint-plugin": "5.16.0",
		"@typescript-eslint/parser": "5.16.0",
		"eslint": "8.11.0",
		"eslint-config-prettier": "8.5.0",
		"eslint-plugin-prettier": "^4.0.0",
		"eslint-plugin-react": "7.29.4",
		"eslint-plugin-react-hooks": "^4.3.0",
		"prettier": "2.6.0",
		"react": "17.0.2",
		"rimraf": "^3.0.2",
		"typescript": "4.6.2"
	},
	"license": "ISC",
	"bugs": {
		"url": "https://github.com/manGoweb/contember-plugins/issues"
	},
	"homepage": "https://github.com/manGoweb/contember-plugins#readme",
	"dependencies": {
		"snappyimg": "^0.5.0"
	}
}
