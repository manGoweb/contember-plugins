{
	"name": "@mangoweb/contember-plugins",
	"version": "0.1.11",
	"description": "Common contember plugins.",
	"files": [
		"dist"
	],
	"main": "dist/index.js",
	"type": "module",
	"private": true,
	"scripts": {
		"test": "npm run eslint:lint",
		"eslint:lint": "eslint \"src/**/*.{ts,tsx}\"",
